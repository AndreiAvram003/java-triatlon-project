<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #f0f0f0; -fx-padding: 20;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.ReportController">

    <!-- VBox pentru a organiza elementele vertical -->
    <VBox spacing="10" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="20">
        <!-- TextField pentru introducerea numÄƒrului de puncte -->

        <!-- TableView existent -->
        <TableView fx:id="tableView" minHeight="-Infinity" minWidth="-Infinity"
                   prefHeight="381.0" prefWidth="663.0" style="-fx-background-color: #ffffff; -fx-border-color: #cccccc; -fx-border-width: 1px; -fx-border-radius: 5;">

            <columns>
                <TableColumn fx:id="tableColumnParticipantName" prefWidth="75.0" text="Name"/>
                <TableColumn fx:id="tableColumnParticipantPoints" prefWidth="75.0" text="Points"/>
            </columns>

            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>


    </VBox>

</AnchorPane>
